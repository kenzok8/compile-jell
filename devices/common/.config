
CONFIG_IPV6=y

CONFIG_LUCI_CSSTIDY=n
CONFIG_AUTOREBUILD=y
CONFIG_AUTOREMOVE=n
CONFIG_ALL=y
CONFIG_SIGNED_PACKAGES=n
CONFIG_SIGNATURE_CHECK=n
CONFIG_XRAY_PROVIDE_V2RAY=y
CONFIG_PACKAGE_dnsmasq=n
CONFIG_v2ray-plugin_INCLUDE_GOPROXY=n
CONFIG_V2RAY_CORE_COMPRESS_UPX=n
CONFIG_XRAY_CORE_COMPRESS_UPX=n

CONFIG_PACKAGE_luci-app-turboacc=y
CONFIG_PACKAGE_TURBOACC_INCLUDE_OFFLOADING=y
CONFIG_PACKAGE_TURBOACC_INCLUDE_SHORTCUT_FE=y
CONFIG_PACKAGE_TURBOACC_INCLUDE_BBR_CCA=y
CONFIG_PACKAGE_TURBOACC_INCLUDE_DNSFORWARDER=n
CONFIG_PACKAGE_TURBOACC_INCLUDE_DNSPROXY=n
CONFIG_PACKAGE_luci-app-passwall_INCLUDE_v2ray-plugin=n

# CONFIG_UA2F_CUSTOM_USER_AGENT=y

CONFIG_NGINX_DAV=y

# daed
CONFIG_DEVEL=y
CONFIG_BPF_TOOLCHAIN_HOST=y
# CONFIG_BPF_TOOLCHAIN_NONE is not set
CONFIG_KERNEL_BPF_EVENTS=y
CONFIG_KERNEL_CGROUP_BPF=y
CONFIG_KERNEL_DEBUG_INFO=y
CONFIG_KERNEL_DEBUG_INFO_BTF=y
# CONFIG_KERNEL_DEBUG_INFO_REDUCED is not set
CONFIG_KERNEL_XDP_SOCKETS=y

CONFIG_PACKAGE_containerd=n

CONFIG_OPENSSL_OPTIMIZE_SPEED=y
CONFIG_OPENSSL_PREFER_CHACHA_OVER_GCM=y
CONFIG_OPENSSL_ENGINE_BUILTIN=y
CONFIG_OPENSSL_ENGINE_BUILTIN_AFALG=y
CONFIG_OPENSSL_ENGINE_BUILTIN_DEVCRYPTO=y

CONFIG_COREMARK_OPTIMIZE_O3=y
CONFIG_COREMARK_ENABLE_MULTITHREADING=y
CONFIG_COREMARK_NUMBER_OF_THREADS=16

CONFIG_LUCI_LANG_zh_Hans=n

# end of 3. Applications

CONFIG_BUSYBOX_CUSTOM=y

CONFIG_IMAGEOPT=y
CONFIG_VERSIONOPT=y
CONFIG_VERSION_REPO="https://op.dllkids.xyz"
CONFIG_VERSION_MANUFACTURER="kenzo'"
CONFIG_VERSION_FILENAMES=n
CONFIG_VERSION_CODE_FILENAMES=n
